        "#function to calculate the gradient\n",
        "def g(x):\n",
        "  return 2*(x-1)\n",
        "\n",
        "#intialize variable x\n",
        "x=3\n",
        "learning_rate=0.001\n",
        "\n",
        "for i in range(5000):\n",
        "  x=x-learning_rate*g(x)\n",
